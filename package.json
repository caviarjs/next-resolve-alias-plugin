  ],
  "engines": {
    "node": ">= 6"
  },
  "ava": {
    "babel": false
  },
  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/caviar-plugin-resolve-alias/issues"
  },
  "devDependencies": {
    "ava": "^1.2.1",
    "codecov": "^3.2.0",
    "nyc": "^13.3.0",
    "eslint-config-ostai": "^1.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint": "^5.14.0"
  },
  "dependencies": {
    "core-util-is": "^1.0.2",
    "err-object": "^4.1.0",
    "object-access": "^2.0.4"
